@model MainWebsite.Models.MaroonMapsModel
@{
    
}
<!DOCTYPE html>
<html>
<head>
    <meta name="viewport" content="initial-scale=1.0, user-scalable=no" />
    <style type="text/css">
        html
        {
            height: 100%;
        }

        body
        {
            height: 100%;
            margin: 0;
            padding: 0;
        }

        #container
        {
            width: 100px;
        }

        #left
        {
            width: 30px;
            float: left;
        }

        #right
        {
            width: 70px;
            float: right;
        }

        #map-canvas
        {
            height: 100%;
        }
    </style>
</head>
<body>
    <div>
        @Html.Partial("_SearchBox")
    </div>

    <div class="container">
        @{
            foreach (var a in Model.Locations)
            {
            <p>
                <a href="/MaroonMaps/Place?title=@a.FormattedAddress&lat=@a.Lattitude&lon=@a.Longitude">@a.FormattedAddress</a>
            </p>
            }
        }
    </div>
</body>
</html>
